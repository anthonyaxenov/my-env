#!/bin/bash
##makedesc: Install KDE Backports + ppa

echo
echo "==============================================="
echo "Installing KDE Backports + ppa..."
echo "==============================================="
echo

sudo add-apt-repository -y ppa:kubuntu-ppa/backports
sudo add-apt-repository -y ppa:kubuntu-ppa/backports-extra
sudo apt update
sudo pkcon update
sudo apt full-upgrade
